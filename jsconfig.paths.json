{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@helpers/*": [
                "./src/helpers/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@img/*": [
                "./src/assets/img/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}